<h1>Hello</h1>
<p>By cristina</p>
<% @questions.each do |q| %>
<div class="media">
  <div class="media-left">
    <a href="#">
      <img class="media-object" src="<%= q.gravatar %>" alt="..." width="80" height="80">
    </a>
  </div>
  <div class="media-body">
    <h4 class="media-heading"><%= q.email %> asked</h4>
    <%= time_ago_in_words q.created_at %> ago
    <br>
    <%= q.body %>
      <div>
        <a href="<%= question_path(q) %>" class="btn btn-success btn-xs">View Answer</a>
      </div>
  </div>
</div>
<% end %>
